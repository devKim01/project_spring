package hello.hellospring.repository;

import javax.sql.DataSource;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.jdbc.core.JdbcTemplate;

public class JdbcTempltaeMemberRepository implements MemberRepository{
	private final JdbcTemplate jdbcTimeplate;
	
	@Autowired
	public JdbcTempltaeMemberRepository(DataSource dataSource) {
		jdbcTimeplate = new JdbcTemplate(dataSource);
		
	}
}
